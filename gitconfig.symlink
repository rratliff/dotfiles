[alias]
	co = checkout
[user]
	name = Bobby Ratliff
	email = ratliff.bobby@gmail.com
[color]
	ui = auto
[push]
	default = simple
[core]
	excludesfile = /Users/Robert/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[commit]
	template = /Users/Robert/.stCommitMsg
